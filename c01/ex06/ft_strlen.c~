#include <stdio.h>

int	ft_strlen(char *str)
{
	int	 a;

	a = 0;
	while (str[a] != '\0')
	{
		a++;
	}
	return (a);
}

int	main(void)
{
	char str[] = "ALİİ, VELİ";
	printf("%d", ft_strlen(str));
	return (0);
}
